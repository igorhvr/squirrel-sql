    <jar href="commons-cli.jar"/>
    <jar href="dom4j.jar"/>
    <jar href="ejb3-persistence.jar"/>
    <jar href="forms.jar"/>
    <jar href="hibernate.jar"/>
    <jar href="hibernate-annotations.jar"/>
    <jar href="hibernate-commons-annotations.jar"/>
    <jar href="hibernate-entitymanager.jar"/>
    <jar href="jxl.jar"/>
    <jar href="log4j.jar"/>
    <jar href="nanoxml.jar"/>
    <jar href="spring-beans.jar"/>
    <jar href="spring-context.jar"/>
    <jar href="spring-context-support.jar"/>
    <jar href="spring-core.jar"/>
