<?xml version="1.0" encoding="utf-8"?>
<jnlp spec="1.0+" codebase="http://www.squirrelsql.org/jws" href="squirrel.jnlp">
  <information>
    <title>SQuirreL SQL Client</title>
    <vendor>SourceForge.net</vendor>
    <homepage href="http://www.squirrelsql.org/"/>
